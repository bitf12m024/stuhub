@using studentshub.Models
@model IEnumerable<User>
@{
    ViewBag.Title = "rep";
}

@{ 
    Layout = "~/Views/Shared/_dashboards.cshtml";
    }
  <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Representatives</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                           Admins
                        </div>
                        <!-- /.panel-heading -->
                          <div class="panel-body">
                            <div class="dataTable_wrapper">
                                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                    <thead>
                                        <tr>
                                            <th>EMAIL</th>
                                            <th>First Name</th>
                                            <th>Last Name</th>
                                            <th>Password</th>
                                            <th>Display Name</th>
                                            <th>Actions</th>
                                        </tr>
                                    </thead>
                 
                                    <tbody>
                                         @foreach (User abc in Model)
                                        {
                                             
                                            <tr class="odd gradeX">
                                                <td>@abc.email</td>
                                                <td>@abc.firstName</td>
                                                <td>@abc.lastName</td>
                                                <td>@abc.password</td>
                                                <td>@abc.display</td>
                                                 <td>
                                                    <a class="btn btn-danger btn-outline btn-circle" href="/Dashboard/dele?id=@abc.UId"><i class="fa fa-times"></i></a>
                                                </td>
                                            </tr>
                                        }
                                        
                                       
                                    </tbody>
            </table>
                            </div>
                            <!-- /.table-responsive -->
                            
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-6 -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /#page-wrapper -->


    <!-- /#wrapper -->
